{
  "manifest_version": 3,
  "name": "Search Things",
  "description": "An extension to automate the search of news",
  "version": "0.1",
  "permissions": ["tabGroups", "storage"],
  "options_page": "options.html",
  "action": {
    "default_popup": "hello.html"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Command+Shift+S"
      }
    }
  }
}
